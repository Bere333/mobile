{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "react-native",
    "lib": [
      "dom",
      "esnext"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "*": [
        "./src/*",
        "./node_modules/*"
      ]
    }
  },
  "include": [
    "./src/**/*",
    "src/components/TreeList/TreeList/.tsx"
  ],
  "extends": "expo/tsconfig.base"
}
